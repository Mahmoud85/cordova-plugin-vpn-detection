<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-vpnchecker" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>cordova-plguin-vpnchecker</name><js-module name="cordova-plugin-vpnchecker" src="www/cordova-plugin-vpnchecker.js"><clobbers target="cordova.plugins.cordova-plguin-vpnchecker" />
</js-module>
<platform name="android">
<config-file parent="/*" target="res/xml/config.xml">
<feature name="cordova-plugin-vpnchecker"><param name="android-package" value="cordova-plugin-vpnchecker.cordova-plugin-vpnchecker" />
</feature>
</config-file><config-file parent="/*" target="AndroidManifest.xml" />
<source-file src="src/android/cordova-plugin-vpnchecker.java" target-dir="src/cordova-plugin-vpnchecker/cordova-plugin-vpnchecker" />
</platform>
<platform name="ios">
<config-file parent="/*" target="config.xml">
<feature name="cordova-plugin-vpnchecker">
<param name="ios-package" value="cordova-plugin-vpnchecker" />
</feature>
</config-file>
<source-file src="src/ios/vpnchecker.swift" />
</platform>
</plugin>